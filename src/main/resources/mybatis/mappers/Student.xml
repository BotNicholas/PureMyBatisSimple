<?xml version = "1.0" encoding = "UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--Now i've specified mapper. MyBatis will automatically create implementation for given mapper interface-->
<mapper namespace = "org.nicholas.mappers.StudentMapper">

    <insert id = "insert" parameterType = "Student">
        INSERT INTO student (NAME, BRANCH, PERCENTAGE, PHONE, EMAIL ) VALUES (#{name}, #{branch}, #{percentage}, #{phone}, #{email});

# needed to obtain generated id
<!--        <selectKey keyProperty = "id" resultType = "int" order = "AFTER">-->
<!--            select last_insert_id() as id-->
<!--        </selectKey>-->
    </insert>

</mapper>
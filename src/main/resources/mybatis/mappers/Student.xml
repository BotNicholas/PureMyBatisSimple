<?xml version = "1.0" encoding = "UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--here we must define mapper interface, but, if such interface is missing you can specify any unique id (namespace name) like shown below -->
<mapper namespace = "StudentNamespace">

    <insert id = "insert" parameterType = "Student">
        INSERT INTO student (NAME, BRANCH, PERCENTAGE, PHONE, EMAIL ) VALUES (#{name}, #{branch}, #{percentage}, #{phone}, #{email});

# needed to obtain generated id
<!--        <selectKey keyProperty = "id" resultType = "int" order = "AFTER">-->
<!--            select last_insert_id() as id-->
<!--        </selectKey>-->
    </insert>

</mapper>